{
  "name": "meta_loan_web",
  "version": "1.0.3",
  "description": "MIB;Loan;Tifi;Xgo;App",
  "author": "weiwei.lu@xgo.one",
  "private": true,
  "license": "ISC",
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "7003"
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": "> 1%,last 2 versions,not ie <= 8",
          "useBuiltIns": "entry",
          "corejs": "3"
        }
      ]
    ]
  },
  "scripts": {
    "dev": "npm run clean:nuxt && cross-env BASE_URL=https://website.xcreditech.com  BASE_HOST=website.xcreditech.com NODE_ENV=development babel-node server/index.js nuxt --watch --analyze",
    "dev:pwa": "cross-env BASE_URL=https://website.xcreditech.com BASE_HOST=website.xcreditech.com NODE_ENV=development nuxt --watch --analyze",
    "build": "cross-env BASE_URL=https://www.website.es BASE_HOST=www.website.es NODE_ENV=production nuxt build --analyze",
    "build:pre": "cross-env BASE_URL=https://www-test.website.es BASE_HOST=www-test.website.es NODE_ENV=production nuxt build --analyze",
    "build:test": "cross-env BASE_URL=https://website.xcreditech.com BASE_HOST=website.xcreditech.com NODE_ENV=development nuxt build --analyze",
    "start": "nuxt start --analyze",
    "generate": "npm run clean:dist && cross-env BASE_URL=https://website.xcreditech.com BASE_HOST=website.xcreditech.com NODE_ENV=production nuxt generate --analyze",
    "deploy": "npm run build",
    "deploy:pre": "npm run build:pre",
    "deploy:test": "npm run build:test",
    "serve": "nodemon server/index.js --exec babel-node --watch",
    "clean:dist": "rimraf dist",
    "clean:lib": "rimraf node_modules",
    "clean:nuxt": "rimraf .nuxt",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --ignore-pattern /static/",
    "fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore . --ignore-pattern /static/"
  },
  "peerDependencies": {
    "webpack": "^4.36.0 || ^5.0.5"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/i18n": "^7.0.3",
    "@nuxtjs/proxy": "^2.1.0",
    "@nuxtjs/workbox": "^3.0.0-beta.16",
    "@nuxtjs/device": "^2.1.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "asmcrypto.js": "^2.3.2",
    "core-js": "3",
    "dayjs": "^1.10.7",
    "element-ui": "^2.15.6",
    "js-cookie": "^3.0.1",
    "jsencrypt": "^3.2.1",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-log4": "^2.3.2",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "nuxt": "^2.0.0",
    "uuid": "^8.3.2",
    "vue-easy-notice-bar": "^1.1.2",
    "vue-fullpage.js": "^0.1.7",
    "vuescroll": "^4.17.3",
    "vuex-persist": "^3.1.3",
    "sa-sdk-javascript": "^1.21.10",
    "vue-gtag": "^1.16.1",
    "workbox-cdn": "^5.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "autoprefixer": "^8.0.0",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^5.0.1",
    "cross-env": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "7.3.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.18.9",
    "pdfjs-dist": "^2.13.216",
    "postcss-pxtorem": "^5.1.1",
    "sass": "~1.32.0",
    "sass-loader": "^10.1.1",
    "scss": "^0.2.4",
    "terser-webpack-plugin": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^8.5.0"
  },
  "browserslist": [
    "defaults",
    "not ie < 11",
    "last 5 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions",
    "Android 4.1",
    "last 3 Android versions",
    "not dead"
  ]
}
